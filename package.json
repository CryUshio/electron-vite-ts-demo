{
  "name": "electron-vite-ts-demo",
  "version": "1.0.0",
  "description": "electron+vue3/react+vite+webpack+ts+tsx+multi-page build demo",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf release dist",
    "start": "run-p renderer-dev electron-pre-dev",
    "renderer-dev": "webpack serve --config ./config/webpack.dev.config.js",
    "renderer-dev:vite": "vite --config ./config/vite.config.ts",
    "renderer-build": "webpack --config ./config/webpack.config.js",
    "electron-pre-dev": "wait-on -d 1000 tcp:3000 && nodemon --exec 'npm run electron-dev'",
    "electron-dev": "tsc -p ./tsconfig.node.json && electron .",
    "electron-build": "tsc -p ./tsconfig.node.json",
    "generate-app": "electron-builder",
    "release": "npm-run-all clean --parallel electron-build renderer-build --serial generate-app"
  },
  "build": {
    "productName": "electron-vue3-ts-demo",
    "appId": "com.electron-vue3-ts-demo",
    "copyright": "MIT",
    "compression": "store",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "!dist/**/*.js.map"
    ],
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/logo.png",
      "uninstallerIcon": "./public/logo.png",
      "installerHeaderIcon": "./public/logo.png",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "ele"
    },
    "win": {
      "icon": "./public/logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "./public/logo.png"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CryUshio/electron-vue3-ts-demo.git"
  },
  "keywords": [
    "electron",
    "vue",
    "vue3",
    "react",
    "vite",
    "webpack",
    "ts",
    "tsx",
    "jsx",
    "multi",
    "multi-page"
  ],
  "author": "CryUshio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CryUshio/electron-vue3-ts-demo/issues"
  },
  "homepage": "https://github.com/CryUshio/electron-vue3-ts-demo#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@swc/core": "^1.3.16",
    "@types/glob": "^8.0.0",
    "@types/nunjucks": "^3.2.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.44",
    "@vue/composition-api": "^1.7.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "babel-preset-vca-jsx": "^0.3.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "20",
    "electron-builder": "^23.6.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^8.0.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "minimist": "^1.2.7",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "nunjucks-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-swc": "^0.2.1",
    "simple-nunjucks-loader": "^3.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "typescript": "4.7.4",
    "vite": "^3.2.3",
    "vue-loader": "^17.0.1",
    "wait-on": "^6.0.1",
    "webpack": "5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "element-plus": "^2.2.20",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "uuid": "^9.0.0",
    "vue": "^3.2.44",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-property-decorator": "^10.0.0-rc.3",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "vuex-class-modules": "^1.3.0"
  }
}
