{
  "name": "electron-vite-ts-demo",
  "version": "1.0.0",
  "description": "electron+vue3/react+vite+webpack+ts+tsx+multi-page build demo",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf release dist",
    "start": "run-p render-dev electron-pre-dev",
    "render-dev": "vite --config ./config/vite.config.ts",
    "render-build": "webpack --config ./config/webpack.config.js",
    "electron-pre-dev": "wait-on -d 1000 tcp:3000 && nodemon --exec 'npm run electron-dev'",
    "electron-dev": "tsc && electron .",
    "electron-build": "tsc",
    "generate-app": "electron-builder",
    "release": "npm-run-all clean --parallel electron-build render-build --serial generate-app"
  },
  "build": {
    "productName": "electron-vue3-ts-demo",
    "appId": "com.electron-vue3-ts-demo",
    "copyright": "MIT",
    "compression": "store",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "!dist/**/*.js.map"
    ],
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/logo.png",
      "uninstallerIcon": "./public/logo.png",
      "installerHeaderIcon": "./public/logo.png",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "ele"
    },
    "win": {
      "icon": "./public/logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "./public/logo.png"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CryUshio/electron-vue3-ts-demo.git"
  },
  "keywords": [
    "electron",
    "vue",
    "vue3",
    "react",
    "vite",
    "webpack",
    "ts",
    "tsx",
    "jsx",
    "multi",
    "multi-page"
  ],
  "author": "CryUshio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CryUshio/electron-vue3-ts-demo/issues"
  },
  "homepage": "https://github.com/CryUshio/electron-vue3-ts-demo#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/glob": "^7.1.3",
    "@types/nunjucks": "^3.1.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vca-jsx": "^0.3.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.3.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.6",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "nunjucks-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "simple-nunjucks-loader": "^3.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "vite-plugin-react": "^4.0.1",
    "wait-on": "^5.2.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^3.3.12",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
